{
  "if": [
    { "==": [{ "var": "discount_scheme" }, "EARLY_BIRD"] },
    {
      "-": [
        {
          "Math.round": {
            "/": [{ "var": "bus_price" }, { "var": "passenger_goal" }]
          }
        },

        {
          "Math.round": {
            "*": [
              {
                "Math.round": {
                  "/": [{ "var": "bus_price" }, { "var": "passenger_goal" }]
                }
              },
              { "/": [{ "var": "early_bird_discount" }, 100] }
            ]
          }
        }
      ]
    },

    { "==": [{ "var": "discount_scheme" }, "CUT_PRICE"] },
    {
      "Math.round": {
        "*": [
          {
            "Math.round": {
              "/": [{ "var": "bus_price" }, { "var": "passenger_goal" }]
            }
          },
          { "/": [{ "-": [100, { "var": "cut_price_discount" }] }, 100] }
        ]
      }
    },

    { "==": [{ "var": "discount_scheme" }, "NONE"] },
    {
      "Math.round": {
        "/": [{ "var": "bus_price" }, { "var": "passenger_goal" }]
      }
    }
  ]
}
